.root {
  background: #333;
  padding: 30px;
  padding-bottom: 80px;
  width: 100%;
  box-sizing: border-box;
  color: #fff;
  position: relative;
}

.list {
  font-size: 0;
}

.item {
  display: inline-block;
  font-size: 16px;
  margin-right: 30px;
  &:last-child {
    margin-right: 0;
  }
}

.subList {

}

.subTitle {
  font-size: 20px;
}

.subItem {
  margin: 15px 0;
}

.copyright {
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 30px;
  text-align: center;
}